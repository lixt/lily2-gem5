////////////////////////////////////////////////////////////////////
//
// The actual LILY2 ISA decoder
//

decode FU default Unknown::unknown () {

    0x0: // FU(0x0).
        decode OPCODE_HI default Unknown::unknown () {

            0x0: //< FU(0b'00) + HI(0b'000).
                decode OPCODE_ME default Unknown::unknown () {

                    0x0: //< ADD.
                        decode FLAG_IMM default Unknown::unknown () {

                            0x0:
                                decode FLAG_VEC default Unknown::unknown () {
                                    0x0: ArithOp::add_a ({{ wd = add (wa, wb); }});
                                    0x1:
                                        decode VEC default Unknown::unknown () {
                                            0x0: ArithVecOp::add_b_4_a ({{ qbd = add_b_4 (qba, qbb); }});
                                            0x1: ArithVecOp::add_h_2_a ({{ dhd = add_h_2 (dha, dhb); }});
                                            0x2: ArithVecOp::add_h_4_a ({{ qhd = add_h_4 (qha, qhb); }});
                                            0x3: ArithVecOp::add_w_2_a ({{ dwd = add_w_2 (dwa, dwb); }});
                                        }
                                }

                            0x1: ArithImmOp::add_i_a ({{ wd = add (wa, i8); }});

                        } //> ADD.

                    0x1: //< ADC.
                        decode FLAG_IMM default Unknown::unknown () {

                            0x0: ArithOp::adc_a      ({{ wd = adc (wa, wb, w_carry); }});
                            0x1: ArithImmOp::adc_i_a ({{ wd = adc (wa, i8, w_carry); }});

                        } //> ADC.

                    0x2: //< SUB.
                        decode FLAG_IMM default Unknown::unknown () {

                            0x0:
                                decode FLAG_VEC default Unknown::unknown () {
                                    0x0: ArithOp::sub_a ({{ wd = sub (wa, wb); }});
                                    0x1:
                                        decode VEC default Unknown::unknown () {
                                            0x0: ArithVecOp::sub_b_4_a ({{ qbd = sub_b_4 (qba, qbb); }});
                                            0x1: ArithVecOp::sub_h_2_a ({{ dhd = sub_h_2 (dha, dhb); }});
                                            0x2: ArithVecOp::sub_h_4_a ({{ qhd = sub_h_4 (qha, qhb); }});
                                            0x3: ArithVecOp::sub_w_2_a ({{ dwd = sub_w_2 (dwa, dwb); }});
                                        }
                                }

                            0x1: ArithImmOp::sub_i_a ({{ wd = sub (wa, i8); }});

                        } //> SUB.

                    0x3: //< SBC.
                        decode FLAG_IMM default Unknown::unknown () {

                            0x0: ArithOp::sbc_a      ({{ wd = sbc (wa, wb, w_carry); }});
                            0x1: ArithImmOp::sbc_i_a ({{ wd = sbc (wa, i8, w_carry); }});

                        } //> SBC.

                    0x4: //< RSB.
                        decode FLAG_IMM default Unknown::unknown () {

                            0x0:
                                decode FLAG_VEC default Unknown::unknown () {
                                    0x0: ArithOp::rsb_a ({{ wd = rsb (wa, wb); }});
                                    0x1:
                                        decode VEC default Unknown::unknown () {
                                            0x0: ArithVecOp::rsb_b_4_a ({{ qbd = rsb_b_4 (qba, qbb); }});
                                            0x1: ArithVecOp::rsb_h_2_a ({{ dhd = rsb_h_2 (dha, dhb); }});
                                            0x2: ArithVecOp::rsb_h_4_a ({{ qhd = rsb_h_4 (qha, qhb); }});
                                            0x3: ArithVecOp::rsb_w_2_a ({{ dwd = rsb_w_2 (dwa, dwb); }});
                                        }
                                }

                            0x1: ArithImmOp::rsb_i_a ({{ wd = rsb (wa, i8); }});

                        } //> RSB.

                    0x5: //< RSC.
                        decode FLAG_IMM default Unknown::unknown () {

                            0x0: ArithOp::rsc_a      ({{ wd = rsc (wa, wb, w_carry); }});
                            0x1: ArithImmOp::rsc_i_a ({{ wd = rsc (wa, i8, w_carry); }});

                        } //> RSC.

                    0x7: //< ABS, NEG, SXB, SXH, ZXB, ZXH.
                        decode OPCODE_LO default Unknown::unknown () {

                            0x0: //< ABS.
                                decode FLAG_VEC default Unknown::unknown () {

                                    0x0: ArithOp::abs_a ({{ wd = abs (wa); }});
                                    0x1:
                                        decode VEC default Unknown::unknown () {
                                            0x0: ArithVecOp::abs_b_4_a ({{ qbd = abs_b_4 (qba); }});
                                            0x1: ArithVecOp::abs_h_2_a ({{ dhd = abs_h_2 (dha); }});
                                            0x2: ArithVecOp::abs_h_4_a ({{ qhd = abs_h_4 (qha); }});
                                            0x3: ArithVecOp::abs_w_2_a ({{ dwd = abs_w_2 (dwa); }});
                                        }

                                } //> ABS.

                            0x1: //< NEG.
                                decode FLAG_VEC default Unknown::unknown () {

                                    0x0: ArithOp::neg_a ({{ wd = abs (wa); }});
                                    0x1:
                                        decode VEC default Unknown::unknown () {
                                            0x0: ArithVecOp::neg_b_4_a ({{ qbd = neg_b_4 (qba); }});
                                            0x1: ArithVecOp::neg_h_2_a ({{ dhd = neg_h_2 (dha); }});
                                            0x2: ArithVecOp::neg_h_4_a ({{ qhd = neg_h_4 (qha); }});
                                            0x3: ArithVecOp::neg_w_2_a ({{ dwd = neg_w_2 (dwa); }});
                                        }

                                } //> NEG.

                            0x2: ArithOp::sxb_a ({{ wd = sxb (wa); }});
                            0x3: ArithOp::sxh_a ({{ wd = sxh (wa); }});
                            0x4: ArithOp::zxb_a ({{ wd = zxb (wa); }});
                            0x5: ArithOp::zxh_a ({{ wd = zxh (wa); }});

                        } //> ABS, NEG, SXB, SXH, ZXB, ZXH.

                } //> FU(0b'00) + HI(0b'000).

            0x1: //< FU(0b'00) + HI(0b'001).
                decode OPCODE_ME default Unknown::unknown () {

                    0x0: //< AND.
                        decode FLAG_IMM default Unknown::unknown () {

                            0x0:
                                decode FLAG_VEC default Unknown::unknown () {
                                    0x0: LogicOp::and_a ({{ wd = bitAnd (wa, wb); }});
                                    0x1:
                                        decode VEC default Unknown::unknown () {
                                            0x0: LogicVecOp::and_b_4_a ({{ qbd = bitAnd_b_4 (qba, qbb); }});
                                            0x1: LogicVecOp::and_h_2_a ({{ dhd = bitAnd_h_2 (dha, dhb); }});
                                            0x2: LogicVecOp::and_h_4_a ({{ qhd = bitAnd_h_4 (qha, qhb); }});
                                            0x3: LogicVecOp::and_w_2_a ({{ dwd = bitAnd_w_2 (dwa, dwb); }});
                                        }
                                }

                            0x1: LogicImmOp::and_i_a ({{ wd = bitAnd (wa, i8); }});

                        } //> AND.

                    0x1: //< NAD.
                        decode FLAG_IMM default Unknown::unknown () {

                            0x0:
                                decode FLAG_VEC default Unknown::unknown () {
                                    0x0: LogicOp::nad_a ({{ wd = bitNad (wa, wb); }});
                                    0x1:
                                        decode VEC default Unknown::unknown () {
                                            0x0: LogicVecOp::nad_b_4_a ({{ qbd = bitNad_b_4 (qba, qbb); }});
                                            0x1: LogicVecOp::nad_h_2_a ({{ dhd = bitNad_h_2 (dha, dhb); }});
                                            0x2: LogicVecOp::nad_h_4_a ({{ qhd = bitNad_h_4 (qha, qhb); }});
                                            0x3: LogicVecOp::nad_w_2_a ({{ dwd = bitNad_w_2 (dwa, dwb); }});
                                        }
                                }

                            0x1: LogicImmOp::nad_i_a ({{ wd = bitNad (wa, i8); }});

                        } //> NAD.

                    0x2: //< ORR.
                        decode FLAG_IMM default Unknown::unknown () {

                            0x0:
                                decode FLAG_VEC default Unknown::unknown () {
                                    0x0: LogicOp::orr_a ({{ wd = bitOrr (wa, wb); }});
                                    0x1:
                                        decode VEC default Unknown::unknown () {
                                            0x0: LogicVecOp::orr_b_4_a ({{ qbd = bitOrr_b_4 (qba, qbb); }});
                                            0x1: LogicVecOp::orr_h_2_a ({{ dhd = bitOrr_h_2 (dha, dhb); }});
                                            0x2: LogicVecOp::orr_h_4_a ({{ qhd = bitOrr_h_4 (qha, qhb); }});
                                            0x3: LogicVecOp::orr_w_2_a ({{ dwd = bitOrr_w_2 (dwa, dwb); }});
                                        }
                                }

                            0x1: LogicImmOp::orr_i_a ({{ wd = bitOrr (wa, i8); }});

                        } //> ORR.

                    0x3: //< NOR.
                        decode FLAG_IMM default Unknown::unknown () {

                            0x0:
                                decode FLAG_VEC default Unknown::unknown () {
                                    0x0: LogicOp::nor_a ({{ wd = bitNor (wa, wb); }});
                                    0x1:
                                        decode VEC default Unknown::unknown () {
                                            0x0: LogicVecOp::nor_b_4_a ({{ qbd = bitNor_b_4 (qba, qbb); }});
                                            0x1: LogicVecOp::nor_h_2_a ({{ dhd = bitNor_h_2 (dha, dhb); }});
                                            0x2: LogicVecOp::nor_h_4_a ({{ qhd = bitNor_h_4 (qha, qhb); }});
                                            0x3: LogicVecOp::nor_w_2_a ({{ dwd = bitNor_w_2 (dwa, dwb); }});
                                        }
                                }

                            0x1: LogicImmOp::nor_i_a ({{ wd = bitNor (wa, i8); }});

                        } //> NOR.

                    0x4: //< XOR.
                        decode FLAG_IMM default Unknown::unknown () {

                            0x0:
                                decode FLAG_VEC default Unknown::unknown () {
                                    0x0: LogicOp::xor_a ({{ wd = bitXor (wa, wb); }});
                                    0x1:
                                        decode VEC default Unknown::unknown () {
                                            0x0: LogicVecOp::xor_b_4_a ({{ qbd = bitXor_b_4 (qba, qbb); }});
                                            0x1: LogicVecOp::xor_h_2_a ({{ dhd = bitXor_h_2 (dha, dhb); }});
                                            0x2: LogicVecOp::xor_h_4_a ({{ qhd = bitXor_h_4 (qha, qhb); }});
                                            0x3: LogicVecOp::xor_w_2_a ({{ dwd = bitXor_w_2 (dwa, dwb); }});
                                        }
                                }

                            0x1: LogicImmOp::xor_i_a ({{ wd = bitXor (wa, i8); }});

                        } //> XOR.

                    0x5: //< NXR.
                        decode FLAG_IMM default Unknown::unknown () {

                            0x0:
                                decode FLAG_VEC default Unknown::unknown () {
                                    0x0: LogicOp::nxr_a ({{ wd = bitNxr (wa, wb); }});
                                    0x1:
                                        decode VEC default Unknown::unknown () {
                                            0x0: LogicVecOp::nxr_b_4_a ({{ qbd = bitNxr_b_4 (qba, qbb); }});
                                            0x1: LogicVecOp::nxr_h_2_a ({{ dhd = bitNxr_h_2 (dha, dhb); }});
                                            0x2: LogicVecOp::nxr_h_4_a ({{ qhd = bitNxr_h_4 (qha, qhb); }});
                                            0x3: LogicVecOp::nxr_w_2_a ({{ dwd = bitNxr_w_2 (dwa, dwb); }});
                                        }
                                }

                            0x1: LogicImmOp::nxr_i_a ({{ wd = bitNxr (wa, i8); }});

                        } //> NXR.

                    0x7: //< NOT.
                        decode OPCODE_LO default Unknown::unknown () {

                            0x0: //< NOT.
                                decode FLAG_IMM default Unknown::unknown () {

                                    0x0:
                                        decode FLAG_VEC default Unknown::unknown () {
                                            0x0: LogicOp::not_a ({{ wd = bitNot (wa); }});
                                            0x1:
                                                decode VEC default Unknown::unknown () {
                                                    0x0: LogicVecOp::not_b_4_a ({{ qbd = bitNot_b_4 (qba); }});
                                                    0x1: LogicVecOp::not_h_2_a ({{ dhd = bitNot_h_2 (dha); }});
                                                    0x2: LogicVecOp::not_h_4_a ({{ qhd = bitNot_h_4 (qha); }});
                                                    0x3: LogicVecOp::not_w_2_a ({{ dwd = bitNot_w_2 (dwa); }});
                                                }
                                        }

                                } //> NOT.

                        } //> NOT.

                } //> FU(0b'00) + HI(0b'001).

            0x2: //< FU(0b'00) + HI(0b'010).
                decode OPCODE_ME default Unknown::unknown () {

                    0x0: //< TGT.
                        decode FLAG_IMM default Unknown::unknown () {

                            0x0:
                                decode FLAG_SIG default Unknown::unknown () {
                                    0x0: TestOp::tgt_u_a ({{ wd = tgt_u (wa, wb); }});
                                    0x1: TestOp::tgt_a   ({{ wd = tgt   (wa, wb); }});
                                }

                            0x1:
                                decode FLAG_SIG default Unknown::unknown () {
                                    0x0: TestImmOp::tgt_u_i_a ({{ wd = tgt_u (wa, i8); }});
                                    0x1: TestImmOp::tgt_i_a   ({{ wd = tgt   (wa, i8); }});
                                }

                        } //> TGT.

                    0x1: //< TGE.
                        decode FLAG_IMM default Unknown::unknown () {

                            0x0:
                                decode FLAG_SIG default Unknown::unknown () {
                                    0x0: TestOp::tge_u_a ({{ wd = tge_u (wa, wb); }});
                                    0x1: TestOp::tge_a   ({{ wd = tge   (wa, wb); }});
                                }

                            0x1:
                                decode FLAG_SIG default Unknown::unknown () {
                                    0x0: TestImmOp::tge_u_i_a ({{ wd = tge_u (wa, i8); }});
                                    0x1: TestImmOp::tge_i_a   ({{ wd = tge   (wa, i8); }});
                                }

                        } //> TGE.

                    0x2: //< TLT.
                        decode FLAG_IMM default Unknown::unknown () {

                            0x0:
                                decode FLAG_SIG default Unknown::unknown () {
                                    0x0: TestOp::tlt_u_a ({{ wd = tlt_u (wa, wb); }});
                                    0x1: TestOp::tlt_a   ({{ wd = tlt   (wa, wb); }});
                                }

                            0x1:
                                decode FLAG_SIG default Unknown::unknown () {
                                    0x0: TestImmOp::tlt_u_i_a ({{ wd = tlt_u (wa, i8); }});
                                    0x1: TestImmOp::tlt_i_a   ({{ wd = tlt   (wa, i8); }});
                                }

                        } //> TLT.

                    0x3: //< TLE.
                        decode FLAG_IMM default Unknown::unknown () {

                            0x0:
                                decode FLAG_SIG default Unknown::unknown () {
                                    0x0: TestOp::tle_u_a ({{ wd = tle_u (wa, wb); }});
                                    0x1: TestOp::tle_a   ({{ wd = tle   (wa, wb); }});
                                }

                            0x1:
                                decode FLAG_SIG default Unknown::unknown () {
                                    0x0: TestImmOp::tle_u_i_a ({{ wd = tle_u (wa, i8); }});
                                    0x1: TestImmOp::tle_i_a   ({{ wd = tle   (wa, i8); }});
                                }

                        } //> TLE.

                    0x4: //< TEQ.
                        decode FLAG_IMM default Unknown::unknown () {

                            0x0: TestOp::teq_a      ({{ wd = teq (wa, wb); }});
                            0x1: TestImmOp::teq_i_a ({{ wd = teq (wa, i8); }});

                        } //> TEQ.

                    0x5: //< TNE.
                        decode FLAG_IMM default Unknown::unknown () {

                            0x0: TestOp::tne_a      ({{ wd = tne (wa, wb); }});
                            0x1: TestImmOp::tne_i_a ({{ wd = tne (wa, i8); }});

                        } //> TNE.

                } //> FU(0b'00) + HI(0b'001).

            0x3: //< FU(0b'00) + HI(0b'011).
                decode OPCODE_ME default Unknown::unknown () {

                    0x0: //< GTO, GTA.
                        decode FLAG_SIG default Unknown::unknown () {

                            0x0:
                                decode FLAG_OA default Unknown::unknown () {

                                    0x0:
                                        decode VEC default Unknown::unknown () {
                                            0x0: TestVecOp::gto_u_b_4_a ({{ wd = gto_u_b_4 (qba, qbb); }});
                                            0x1: TestVecOp::gto_u_h_2_a ({{ wd = gto_u_h_2 (dha, dhb); }});
                                            0x2: TestVecOp::gto_u_h_4_a ({{ wd = gto_u_h_4 (qha, qhb); }});
                                            0x3: TestVecOp::gto_u_w_2_a ({{ wd = gto_u_w_2 (dwa, dwb); }});
                                        }

                                    0x1:
                                        decode VEC default Unknown::unknown () {
                                            0x0: TestVecOp::gta_u_b_4_a ({{ wd = gta_u_b_4 (qba, qbb); }});
                                            0x1: TestVecOp::gta_u_h_2_a ({{ wd = gta_u_h_2 (dha, dhb); }});
                                            0x2: TestVecOp::gta_u_h_4_a ({{ wd = gta_u_h_4 (qha, qhb); }});
                                            0x3: TestVecOp::gta_u_w_2_a ({{ wd = gta_u_w_2 (dwa, dwb); }});
                                        }
                                }

                            0x1:
                                decode FLAG_OA default Unknown::unknown () {

                                    0x0:
                                        decode VEC default Unknown::unknown () {
                                            0x0: TestVecOp::gto_b_4_a ({{ wd = gto_b_4 (qba, qbb); }});
                                            0x1: TestVecOp::gto_h_2_a ({{ wd = gto_h_2 (dha, dhb); }});
                                            0x2: TestVecOp::gto_h_4_a ({{ wd = gto_h_4 (qha, qhb); }});
                                            0x3: TestVecOp::gto_w_2_a ({{ wd = gto_w_2 (dwa, dwb); }});
                                        }

                                    0x1:
                                        decode VEC default Unknown::unknown () {
                                            0x0: TestVecOp::gta_b_4_a ({{ wd = gta_b_4 (qba, qbb); }});
                                            0x1: TestVecOp::gta_h_2_a ({{ wd = gta_h_2 (dha, dhb); }});
                                            0x2: TestVecOp::gta_h_4_a ({{ wd = gta_h_4 (qha, qhb); }});
                                            0x3: TestVecOp::gta_w_2_a ({{ wd = gta_w_2 (dwa, dwb); }});
                                        }
                                }

                        } //> GTO, GTA.

                    0x1: //< GEO, GEA.
                        decode FLAG_SIG default Unknown::unknown () {

                            0x0:
                                decode FLAG_OA default Unknown::unknown () {

                                    0x0:
                                        decode VEC default Unknown::unknown () {
                                            0x0: TestVecOp::geo_u_b_4_a ({{ wd = geo_u_b_4 (qba, qbb); }});
                                            0x1: TestVecOp::geo_u_h_2_a ({{ wd = geo_u_h_2 (dha, dhb); }});
                                            0x2: TestVecOp::geo_u_h_4_a ({{ wd = geo_u_h_4 (qha, qhb); }});
                                            0x3: TestVecOp::geo_u_w_2_a ({{ wd = geo_u_w_2 (dwa, dwb); }});
                                        }

                                    0x1:
                                        decode VEC default Unknown::unknown () {
                                            0x0: TestVecOp::gea_u_b_4_a ({{ wd = gea_u_b_4 (qba, qbb); }});
                                            0x1: TestVecOp::gea_u_h_2_a ({{ wd = gea_u_h_2 (dha, dhb); }});
                                            0x2: TestVecOp::gea_u_h_4_a ({{ wd = gea_u_h_4 (qha, qhb); }});
                                            0x3: TestVecOp::gea_u_w_2_a ({{ wd = gea_u_w_2 (dwa, dwb); }});
                                        }
                                }

                            0x1:
                                decode FLAG_OA default Unknown::unknown () {

                                    0x0:
                                        decode VEC default Unknown::unknown () {
                                            0x0: TestVecOp::geo_b_4_a ({{ wd = geo_b_4 (qba, qbb); }});
                                            0x1: TestVecOp::geo_h_2_a ({{ wd = geo_h_2 (dha, dhb); }});
                                            0x2: TestVecOp::geo_h_4_a ({{ wd = geo_h_4 (qha, qhb); }});
                                            0x3: TestVecOp::geo_w_2_a ({{ wd = geo_w_2 (dwa, dwb); }});
                                        }

                                    0x1:
                                        decode VEC default Unknown::unknown () {
                                            0x0: TestVecOp::gea_b_4_a ({{ wd = gea_b_4 (qba, qbb); }});
                                            0x1: TestVecOp::gea_h_2_a ({{ wd = gea_h_2 (dha, dhb); }});
                                            0x2: TestVecOp::gea_h_4_a ({{ wd = gea_h_4 (qha, qhb); }});
                                            0x3: TestVecOp::gea_w_2_a ({{ wd = gea_w_2 (dwa, dwb); }});
                                        }
                                }

                        } //> GEO, GEA.

                    0x2: //< LTO, LTA.
                        decode FLAG_SIG default Unknown::unknown () {

                            0x0:
                                decode FLAG_OA default Unknown::unknown () {

                                    0x0:
                                        decode VEC default Unknown::unknown () {
                                            0x0: TestVecOp::lto_u_b_4_a ({{ wd = lto_u_b_4 (qba, qbb); }});
                                            0x1: TestVecOp::lto_u_h_2_a ({{ wd = lto_u_h_2 (dha, dhb); }});
                                            0x2: TestVecOp::lto_u_h_4_a ({{ wd = lto_u_h_4 (qha, qhb); }});
                                            0x3: TestVecOp::lto_u_w_2_a ({{ wd = lto_u_w_2 (dwa, dwb); }});
                                        }

                                    0x1:
                                        decode VEC default Unknown::unknown () {
                                            0x0: TestVecOp::lta_u_b_4_a ({{ wd = lta_u_b_4 (qba, qbb); }});
                                            0x1: TestVecOp::lta_u_h_2_a ({{ wd = lta_u_h_2 (dha, dhb); }});
                                            0x2: TestVecOp::lta_u_h_4_a ({{ wd = lta_u_h_4 (qha, qhb); }});
                                            0x3: TestVecOp::lta_u_w_2_a ({{ wd = lta_u_w_2 (dwa, dwb); }});
                                        }
                                }

                            0x1:
                                decode FLAG_OA default Unknown::unknown () {

                                    0x0:
                                        decode VEC default Unknown::unknown () {
                                            0x0: TestVecOp::lto_b_4_a ({{ wd = lto_b_4 (qba, qbb); }});
